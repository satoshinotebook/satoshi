{
    "version": 1,
    "routes": [
      {
        "src": "/content/(.*)\\.html",
        "dst": "/$1"
      },
      {
        "src": "/(.*)",
        "dst": "/content/$1.html",
        "check": true
      },
      {
        "src": "/(.*)",
        "dst": "/index.html",
        "status": 200
      }
    ]
  }