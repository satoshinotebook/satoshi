{
    "version": 1,
    "include": [
      "/*"
    ],
    "exclude": [],
    "rules": [
      {
        "type": "rewrite",
        "source": "/reality",
        "destination": "/content/reality.html"
      },
      {
        "type": "rewrite",
        "source": "/scarcity",
        "destination": "/content/scarcity.html"
      },
      {
        "type": "rewrite",
        "source": "/content/*",
        "destination": "/:splat"
      }
    ]
  }